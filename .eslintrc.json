{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "extends": [
        "airbnb-base",
        "airbnb-typescript/base"
    ],
    "rules": {
        "max-len": [
            "error",
            {
                "code": 120
            }
        ],
        "no-plusplus": "off",
        "no-continue": "off"
    }
}